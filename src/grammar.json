{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "haproxy",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "section"
          },
          {
            "type": "SYMBOL",
            "name": "directive_line"
          },
          {
            "type": "SYMBOL",
            "name": "blank_line"
          }
        ]
      }
    },
    "section": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "section_header_line"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "directive_line"
            }
          }
        ]
      }
    },
    "section_header_line": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "global"
            },
            {
              "type": "SYMBOL",
              "name": "newline"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "defaults"
            },
            {
              "type": "SYMBOL",
              "name": "newline"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "STRING",
                  "value": "frontend"
                },
                {
                  "type": "STRING",
                  "value": "backend"
                },
                {
                  "type": "STRING",
                  "value": "listen"
                }
              ]
            },
            {
              "type": "SYMBOL",
              "name": "identifier"
            },
            {
              "type": "SYMBOL",
              "name": "newline"
            }
          ]
        }
      ]
    },
    "directive_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "word"
          }
        },
        {
          "type": "SYMBOL",
          "name": "newline"
        }
      ]
    },
    "blank_line": {
      "type": "SYMBOL",
      "name": "newline"
    },
    "identifier": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[A-Za-z0-9-_]+"
      }
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "#"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    },
    "word": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[^\\s#\\r\\n]+"
      }
    },
    "newline": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\r?\\n"
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[ \\t]+"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
